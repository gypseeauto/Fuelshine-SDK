<?xml version="1.0" encoding="utf-8"?>

                  <androidx.cardview.widget.CardView
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        app:cardCornerRadius="8dp"
                        android:id="@+id/speed_card"
                        android:layout_marginTop="10dp"
                        app:cardUseCompatPadding="true"
                        android:layout_height="wrap_content">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/main_layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/card_speed"
                            android:elevation="16dp"
                            >

                            <LinearLayout
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                android:id="@+id/linear_ll"
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                >

                                <RelativeLayout
                                    xmlns:android="http://schemas.android.com/apk/res/android"
                                    android:id="@+id/icons"
                                    android:layout_width="60dp"
                                    android:layout_height="40dp"
                                    android:layout_marginStart="15dp"
                                    android:clipToOutline="true"
                                    android:background="@drawable/icons1"
                                    >
                                    <ImageView
                                        android:id="@+id/img"
                                        android:layout_width="23dp"
                                        android:layout_height="22dp"
                                        android:layout_centerInParent="true"
                                        />
                                </RelativeLayout>

                                <TextView
                                    android:id="@+id/speeding_text"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/poppins_semibold"
                                    android:text="@string/speeding"
                                    android:textSize="14sp"
                                    android:textStyle="bold"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:lineSpacingExtra="-3sp"
                                    android:layout_marginStart="20dp"
                                    android:layout_gravity="center_vertical"
                                    android:lineHeight="18sp"
                                    android:gravity="top"
                                    />




                            </LinearLayout>


                            <RelativeLayout
                                android:id="@+id/frame_275"
                                app:layout_constraintTop_toBottomOf="@id/linear_ll"
                                android:layout_width="335dp"
                                android:layout_height="38dp"
                                android:gravity="center_vertical"
                                android:layout_marginTop="20dp"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                android:background="@drawable/frame_275"
                                >

                                <TextView
                                    android:id="@+id/minutes_of"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentTop="true"
                                    android:layout_alignParentBottom="true"
                                    android:gravity="center_vertical"
                                    android:text="@string/minutes_of_speeding"
                                    android:textSize="13sp"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:lineHeight="20sp"
                                    android:fontFamily="@font/poppins"
                                    android:layout_alignParentStart="true"
                                    android:layout_marginStart="10dp"
                                    />


                                <TextView
                                    android:id="@+id/value_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"

                                    android:layout_alignParentBottom="true"
                                    android:layout_alignParentTop="true"
                                    android:fontFamily="@font/digital_display"
                                    android:textSize="24sp"
                                    android:textColor="@color/colorPrimary"
                                    android:layout_alignParentEnd="true"
                                    android:lineHeight="16sp"
                                    android:layout_marginEnd="10dp"
                                    android:gravity="center_vertical |center_horizontal"                                />



                            </RelativeLayout>


                            <RelativeLayout
                                android:id="@+id/frame_276"
                                app:layout_constraintTop_toBottomOf="@id/frame_275"
                                android:layout_width="335dp"
                                android:layout_height="38dp"
                                android:gravity="center_vertical"
                                android:layout_marginTop="10dp"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                android:background="@drawable/frame_275"
                                >

                                <TextView
                                    android:id="@+id/hours_of"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentTop="true"
                                    android:layout_alignParentBottom="true"
                                    android:gravity="center_vertical"
                                    android:text="@string/hours_of_travel"
                                    android:textSize="13sp"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:lineHeight="20sp"
                                    android:fontFamily="@font/poppins"
                                    android:layout_alignParentStart="true"
                                    android:layout_marginStart="10dp"
                                    />


                                <TextView
                                    android:id="@+id/value_2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:layout_alignParentTop="true"
                                    android:fontFamily="@font/digital_display"
                                    android:textSize="24sp"
                                    android:textColor="@color/colorPrimary"
                                    android:layout_alignParentEnd="true"
                                    android:lineHeight="16sp"
                                    android:layout_marginEnd="10dp"
                                    android:gravity="center_vertical |center_horizontal"
                                    />



                            </RelativeLayout>



                            <LinearLayout
                                app:layout_constraintTop_toBottomOf="@id/frame_276"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                android:layout_marginTop="20dp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                android:layout_marginBottom="15dp"
                                android:id="@+id/improve_layout"
                                android:layout_width="wrap_content"
                                android:layout_height="32dp"
                                android:background="@drawable/btn_reglas2"
                                android:orientation="horizontal" >



                                <ImageView
                                    android:layout_marginStart="10dp"
                                    android:src="@drawable/fs_info"
                                    android:id="@+id/ic_round_in"
                                    android:layout_width="24dp"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="24dp"
                                    android:clipToOutline="true"
                                    />

                                <TextView
                                    android:id="@+id/general_rul"
                                    android:layout_width="wrap_content"
                                    android:layout_height="20dp"
                                    android:textSize="13sp"
                                    android:layout_gravity="center_vertical"
                                    android:textColor="@color/dark_blue"
                                    android:text="@string/see_how_you_can_improve"
                                    android:lineHeight="20sp"
                                    android:layout_marginEnd="7dp"
                                    android:fontFamily="@font/poppins"
                                    android:gravity="center_horizontal|top"
                                    />





                            </LinearLayout>







                        </androidx.constraintlayout.widget.ConstraintLayout>




                    </androidx.cardview.widget.CardView>


